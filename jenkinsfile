pipeline {
	agent any
	stages {
		stage('Compile Stage'){
			steps{
				withMaven(maven : 'mymaven') {
					sh 'mvn clean compile'
				}
			}
		}
		stage('Testing Stage'){
			steps{
				withMaven(mavne : 'mymaven'){
					sh 'mvn test'
				}
			}
		}
		stage('Deploy Stage'){
			steps{
				withMaven(maven : 'mymaven'){
					sh 'mvn deploy'
				}
			}
		}
	}
}
